{
    "name": "bporto94/mappable-models",
    "description": "A package of classes that help to fix some common problems found in laravel+oracle architecture, using eloquent models.",
    "type": "library",
    "authors": [
      {
        "name": "Philippe Vanzin Moreira",
        "email": "pvanzinmoreira@gmail.com"
      },
      {
        "name": "Igor Moraes",
        "email": "igor.sgm@gmail.com"
      },
      {
        "name": "Wellington Alves",
        "email": "wellington@reddes.com.br"
      }
    ],
    "require": {
        "sofa/eloquence": "^5.6",
    	"sofa/eloquence-base":"^5.6"
    },
    "autoload": {
        "psr-4": {
            "Bporto94\\MappableModels\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bporto94\\MappableModels\\CoreServiceProvider"
            ],
            "aliases": {
                "Core": "MappableModels\\CoreFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
